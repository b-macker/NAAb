# Test default parameters
fn greet(name: string = "World") -> string {
    return "Hello, " + name
}

fn add(a: int, b: int = 10) -> int {
    return a + b
}

main {
    print("Testing default parameters...")

    # Test 1: Call with default
    let msg1 = greet()
    print(msg1)  # Should print: Hello, World

    # Test 2: Call with argument
    let msg2 = greet("Alice")
    print(msg2)  # Should print: Hello, Alice

    # Test 3: Partial defaults
    let sum1 = add(5)
    print(sum1)  # Should print: 15

    # Test 4: All arguments
    let sum2 = add(5, 20)
    print(sum2)  # Should print: 25

    print("All tests complete!")
}
