// Integration Test: Multi-file application - Math library
// Tests: import/export, module resolution, function calls across files

// This file imports math utilities from a separate module
import {add, multiply, factorial} from "./modules/math_utils.naab"

// Test 1: Import basic functions
let result1 = add(5, 3)
if (result1 != 8) {
    throw "FAIL: add(5, 3) should return 8, got " + result1
}
print("PASS: add function imported and working")

// Test 2: Import and chain operations
let result2 = multiply(add(2, 3), 4)
if (result2 != 20) {
    throw "FAIL: multiply(add(2, 3), 4) should return 20, got " + result2
}
print("PASS: function composition across imports")

// Test 3: Import recursive function
let result3 = factorial(5)
if (result3 != 120) {
    throw "FAIL: factorial(5) should return 120, got " + result3
}
print("PASS: recursive function imported and working")

print("âœ“ All multi-file math tests passed")
