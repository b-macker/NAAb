} else {
    print("  ✗ FAIL")
}

let result19 = (a > b) || (b < c)
print("  (10 > 20) || (20 < 30) = " + result19.toString())
if (result19) {
    print("  ✓ PASS")
} else {
    print("  ✗ FAIL")
}

let result20 = !(a > b) && (c > b)
print("  !(10 > 20) && (30 > 20) = " + result20.toString())
if (result20) {
