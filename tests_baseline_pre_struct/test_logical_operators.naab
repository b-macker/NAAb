# Test 4: Combined expressions
print("Test 4: Combined expressions")
let age = 25
let score = 85

let result11 = age >= 18 && score > 80
print("  age(25) >= 18 && score(85) > 80 = " + result11.toString())
if (result11) {
    print("  ✓ PASS")
} else {
    print("  ✗ FAIL")
}

let result12 = age < 18 || score > 80
print("  age(25) < 18 || score(85) > 80 = " + result12.toString())
if (result12) {
    print("  ✓ PASS")
} else {
    print("  ✗ FAIL")
}

let valid = true
let result13 = !valid || score > 90
print("  !valid(true) || score(85) > 90 = " + result13.toString())
if (!result13) {
    print("  ✓ PASS")
} else {
    print("  ✗ FAIL")
}

print("")

# Test 5: Operator precedence
print("Test 5: Operator precedence")
# ! has highest precedence, then &&, then ||
let result14 = !false && true
print("  !false && true = " + result14.toString())
if (result14) {
    print("  ✓ PASS (! has higher precedence than &&)")
} else {
    print("  ✗ FAIL")
}

let result15 = true || false && false
print("  true || false && false = " + result15.toString())
if (result15) {
    print("  ✓ PASS (&& has higher precedence than ||)")
} else {
    print("  ✗ FAIL")
}

print("")

# Test 6: Short-circuit evaluation for AND
print("Test 6: Short-circuit evaluation for AND")
print("  Testing: false && (this should not execute)")
let executed = false
let result16 = false && (executed = true)
print("  executed flag = " + executed.toString())
if (!executed) {
    print("  ✓ PASS (right side not evaluated when left is false)")
} else {
    print("  ✗ FAIL (right side was evaluated when it shouldn't be)")
}

print("")

# Test 7: Short-circuit evaluation for OR
print("Test 7: Short-circuit evaluation for OR")
print("  Testing: true || (this should not execute)")
let executed2 = false
let result17 = true || (executed2 = true)
print("  executed2 flag = " + executed2.toString())
if (!executed2) {
    print("  ✓ PASS (right side not evaluated when left is true)")
} else {
    print("  ✗ FAIL (right side was evaluated when it shouldn't be)")
