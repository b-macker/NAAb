# Comprehensive test for array element assignment

main {
    print("=== Comprehensive Array Assignment Tests ===")
    print("")

    # Test 1: Basic list assignment
    print("[Test 1] Basic List Assignment")
    let numbers = [1, 2, 3, 4, 5]
    print("  Original: ", numbers)
    numbers[0] = 100
    numbers[4] = 500
    print("  After assignments: ", numbers)
    print("  OK")
    print("")

    # Test 2: Dictionary assignment
    print("[Test 2] Dictionary Assignment")
    let person = {"name": "Alice", "age": "30"}
    print("  Original: ", person)
    person["age"] = "31"
    person["city"] = "NYC"
    print("  After assignments: ", person)
    print("  OK")
    print("")

    # Test 3: Assignment in loop
    print("[Test 3] Assignment in Loop")
    let arr = [0, 0, 0, 0, 0]
    print("  Original: ", arr)
    let i = 0
    while i < 5 {
        arr[i] = i * 10
        i = i + 1
    }
    print("  After loop: ", arr)
    print("  OK")
    print("")

    # Test 4: Nested assignment
    print("[Test 4] Multiple Assignments")
    let data = [10, 20, 30]
    data[0] = 99
    data[1] = 88
    data[2] = 77
    print("  Result: ", data)
    print("  OK")
    print("")

    # Test 5: Assignment with expressions
    print("[Test 5] Assignment with Expressions")
    let vals = [1, 2, 3]
    vals[0] = vals[1] + vals[2]
    print("  vals[0] = vals[1] + vals[2]: ", vals)
    print("  OK")
    print("")

    print("=== All Tests Passed! ===")
}
