# Test ISS-009: Regex Module (avoiding 'match' keyword)
# Try functions that aren't reserved keywords

use regex as re

main {
    print("=== Testing ISS-009: Regex Module (v2) ===")

    let text = "Hello World 123"

    # Test 1: re.search (not a keyword)
    print("Test 1: re.search")
    let s = re.search(text, "World")
    print("  Search result:", s)

    # Test 2: re.find_all (not a keyword)
    print("Test 2: re.find_all")
    let all = re.find_all(text, "\\w+")
    print("  Find all result:", all)

    # Test 3: re.replace (not a keyword)
    print("Test 3: re.replace")
    let replaced = re.replace(text, "World", "Universe")
    print("  Replace result:", replaced)

    print("VERDICT: Testing if regex module works at all...")
}
