# Test Phase 2.4.4: Type Inference - Final Demonstration

# Return type inference - no explicit return types!
fn getNumber() {
    return 42
}

fn getMessage() {
    return "Hello, Type Inference!"
}

fn getDecimal() {
    return 3.14159
}

fn checkFlag() {
    return true
}

fn makeList() {
    return [10, 20, 30]
}

main {
    print("=== Type Inference Demonstration ===")
    print("")
    print("All functions above have NO explicit return types!")
    print("The types are automatically inferred:")
    print("")
    print("Check the [INFO] messages above for inferred types:")
    print("  - getNumber() inferred as: int")
    print("  - getMessage() inferred as: string")
    print("  - getDecimal() inferred as: float")
    print("  - checkFlag() inferred as: bool")
    print("  - makeList() inferred as: list")
    print("")
    print("=== Type Inference Working! ===")
}
