# Test JavaScript traceback extraction (Phase 4.2.3)
# This program intentionally raises a JavaScript exception to test stack tracing

# Define JavaScript functions that will raise an exception
javascript {{{
function divide_numbers(a, b) {
    // Will throw TypeError if b is 0
    if (b === 0) {
        throw new Error("Division by zero");
    }
    return a / b;
}

function call_divide() {
    // Calls divide with zero - triggers exception
    return divide_numbers(10, 0);
}
}}}

main {
    print("=== Testing JavaScript Stack Trace Extraction ===")
    print()

    # This will raise Error in JavaScript
    # Stack trace should show: main → call_divide → divide_numbers
    let result = call_divide()

    print("Result:", result)
}
