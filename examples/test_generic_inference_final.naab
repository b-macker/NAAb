# Test Phase 2.4.4 Phase 3: Generic Argument Inference

# Generic identity function - returns what you give it
fn identity<T>(x: T) -> T {
    return x
}

main {
    print("=== Generic Argument Inference Demonstration ===")
    print("")
    print("The identity<T> function is generic.")
    print("When you call it, T is automatically inferred!")
    print("")
    print("Check the [INFO] messages above for:")
    print("  - identity(42) should infer T = int")
    print("  - identity(\"test\") should infer T = string")
    print("")

    # These calls should trigger generic type inference
    let num = identity(42)
    let str = identity("hello")
    let flag = identity(true)

    print("=== Generic Inference Complete! ===")
}
