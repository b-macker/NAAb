main {
    print("=== Multi-line Code Test for ALL Languages ===\n\n")

    // 1. JavaScript
    print("1. JavaScript:\n")
    let js = <<javascript
const a = 10;
const b = 20;
a + b
    >>
    print("   Result: ", js, "\n\n")

    // 2. Python
    print("2. Python:\n")
    let py = <<python
x = 15
y = 25
x * y
    >>
    print("   Result: ", py, "\n\n")

    // 3. Shell
    print("3. Shell:\n")
    let sh = <<bash
A=100
B=200
expr $A + $B
    >>
    print("   Result: ", sh, "\n\n")

    // 4. C++ (simple calculation)
    print("4. C++ (compiled):\n")
    let cpp = <<cpp
int x = 30;
int y = 40;
x + y
    >>
    print("   Result: ", cpp, "\n\n")

    print("=== Core Languages Test Complete ===\n")
    print("JavaScript, Python, Shell, and C++ all support multi-line code!\n")
}
