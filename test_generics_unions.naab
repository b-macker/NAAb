// Test: Check if generics and union types already work

main {
    print("=== Testing Type System Features ===\n")

    // Test 1: Union types
    let value1: int | string = 42
    print("Test 1 - Union type (int): ", value1, "\n")

    let value2: int | string = "hello"
    print("Test 2 - Union type (string): ", value2, "\n")

    // Test 3: Generic list
    let numbers: list<int> = [1, 2, 3]
    print("Test 3 - List<int>: ", numbers, "\n")

    // Test 4: Generic dict
    let scores: dict<string, int> = {
        "alice": 95,
        "bob": 87
    }
    print("Test 4 - Dict<string, int>: ", scores, "\n")

    print("=== Tests complete ===\n")
}
