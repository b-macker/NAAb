main {
    print("=== Testing try/catch ===\n")

    // Test 1: Basic try/catch
    try {
        print("Test 1: In try block\n")
        let x = 5
        print("x = ", x, "\n")
    } catch (e) {
        print("Test 1: Caught error\n")
    }

    // Test 2: Try/catch with throw
    try {
        print("Test 2: Before throw\n")
        throw "Test error"
        print("Test 2: After throw (should not print)\n")
    } catch (e) {
        print("Test 2: Caught: ", e, "\n")
    }

    print("=== try/catch tests complete ===\n")
}
