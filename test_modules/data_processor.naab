use utils.string_ops

// Import sibling module
use math_utils

export fn process_number(n: int) -> int {
    // Use math_utils from imported module
    return math_utils.add(n, 10)
}

export fn process_string(s: string) -> string {
    // Use string_ops from imported nested module
    return string_ops.concat(s, " processed")
}

export fn process_both(n: int, s: string) -> string {
    let num = process_number(n)
    let str = process_string(s)
    // This demonstrates using multiple dependencies
    return str  // In real implementation, would format with number
}
