struct Person {
    name: string,
    age: int
}

main {
    let p = new Person {
        name: "Alice",
        age: 30
    }

    print("Testing struct to JavaScript:\n")

    let result = <<javascript[p]
JSON.stringify({
    name: p.name,
    age: p.age
});
    >>

    print("Result: ", result, "\n")
}
