# Test array append

use array as array

main {
    print("Testing array append")

    let my_list = []
    print("Empty list: ", my_list)

    # Try array.push
    let list2 = array.push(my_list, 10)
    print("After push(10): ", list2)

    let list3 = array.push(list2, 20)
    print("After push(20): ", list3)

    print("Complete")
}
