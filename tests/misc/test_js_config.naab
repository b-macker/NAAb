use json as json

main {
    let config_data = <<javascript
const monitor_config = {
    check_interval_seconds: 60,
    alert_threshold_cpu: 80.0,
    alert_threshold_memory: 85.0,
    alert_threshold_disk: 90.0,
    webhook_url: "https://hooks.slack.com/services/YOUR/WEBHOOK/URL",
    log_retention_days: 30
};
JSON.stringify(monitor_config);
    >>

    print("Config data: ", config_data, "\n")

    let parsed = json.parse(config_data)
    print("Parsed check_interval: ", parsed.check_interval_seconds, "\n")
}
