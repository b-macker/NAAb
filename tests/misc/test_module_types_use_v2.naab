// Test ISS-024 fix: module.Type syntax in function parameters
use test_module_types_def as types

// ISS-024 Fix Test: Use module-qualified type in parameter
function process_config(config: types.Config) {
    print("Processing config: ", config.name, "\n")
    print("Port: ", config.port, "\n")
}

function create_result(success: bool, msg: string) -> types.Result {
    return new types.Result {
        success: success
        message: msg
    }
}

main {
    print("=== ISS-024 Module Type Syntax Test ===\n")

    let my_config = new types.Config {
        name: "test-server"
        port: 8080
    }

    process_config(my_config)

    let result = create_result(true, "Success!")
    print("Result: ", result.success, " - ", result.message, "\n")
}
