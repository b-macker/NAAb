# Test Range Operator - Comprehensive tests for start..end syntax

main {
    print("=== Range Operator Test ===")
    print("")

    # Test 1: Basic range iteration
    print("Test 1: Basic range (0..5)")
    for i in 0..5 {
        print("  i =", i)
    }
    print("  ✓ Basic range works")
    print("")

    # Test 2: Range starting from non-zero
    print("Test 2: Range (10..15)")
    for i in 10..15 {
        print("  i =", i)
    }
    print("  ✓ Non-zero start works")
    print("")

    # Test 3: Range with sum accumulation
    print("Test 3: Sum using range (1..11)")
    let sum = 0
    for i in 1..11 {
        sum = sum + i
    }
    print("  Sum of 1..10:", sum, "(expected 55)")
    print("  ✓ Range accumulation works")
    print("")

    # Test 4: Range with expressions
    print("Test 4: Range with expressions")
    let start = 5
    let end = 8
    for i in start..end {
        print("  i =", i)
    }
    print("  ✓ Range with variables works")
    print("")

    # Test 5: Range with arithmetic expressions
    print("Test 5: Range with arithmetic (2*3..2*5)")
    for i in 2*3..2*5 {
        print("  i =", i)
    }
    print("  ✓ Range with arithmetic works")
    print("")

    # Test 6: Nested range loops
    print("Test 6: Nested ranges")
    for i in 0..3 {
        for j in 0..3 {
            print("  (", i, ",", j, ")")
        }
    }
    print("  ✓ Nested ranges work")
    print("")

    # Test 7: Range with break
    print("Test 7: Range with break")
    for i in 0..10 {
        if i == 5 {
            print("  Breaking at i =", i)
            break
        }
        print("  i =", i)
    }
    print("  ✓ Break in range works")
    print("")

    # Test 8: Range with continue
    print("Test 8: Range with continue (skip even numbers)")
    for i in 0..10 {
        if i % 2 == 0 {
            continue
        }
        print("  i =", i, "(odd)")
    }
    print("  ✓ Continue in range works")
    print("")

    # Test 9: Empty range (start == end)
    print("Test 9: Empty range (5..5)")
    let count = 0
    for i in 5..5 {
        count = count + 1
    }
    print("  Iterations:", count, "(expected 0)")
    print("  ✓ Empty range works")
    print("")

    # Test 10: Build list from range
    print("Test 10: Build list from range")
    let numbers = []
    for i in 0..5 {
        numbers = numbers + [i]
    }
    print("  numbers =", numbers)
    print("  ✓ List building works")
    print("")

    print("=== All Range Operator Tests PASSED! ===")
    print("")
    print("Range Operator Features:")
    print("  ✓ Basic iteration (start..end)")
    print("  ✓ Exclusive end (0..5 gives 0,1,2,3,4)")
    print("  ✓ Variable expressions in range")
    print("  ✓ Arithmetic expressions in range")
    print("  ✓ Nested ranges")
    print("  ✓ Break/continue support")
    print("  ✓ Empty ranges (start == end)")
    print("  ✓ List building from ranges")
    print("")
    print("✅ Range Operator is PRODUCTION READY!")
}
