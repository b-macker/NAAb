main {
    print("=== Test Pipeline Operator with Newlines ===")

    # Test 1: Same line (should always work)
    print("Test 1: Pipeline on same line")
    let r1 = 10 |> add_five |> double
    print("  Result:", r1, "(expected 30)")

    # Test 2: Newline before first operator
    print("Test 2: Newline before |>")
    let r2 = 20
        |> add_five
        |> double
    print("  Result:", r2, "(expected 50)")

    # Test 3: Mixed formatting
    print("Test 3: Mixed newlines")
    let r3 = 5 |> add_five
        |> double
        |> add_five
    print("  Result:", r3, "(expected 25)")

    print("âœ“ All pipeline tests passed!")
}

fn add_five(x: int) -> int {
    return x + 5
}

fn double(x: int) -> int {
    return x * 2
}
