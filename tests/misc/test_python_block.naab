// Simple test for Python block execution

main {
    print("Testing Python block execution\n")

    let test_url = "https://example.com"
    let result: string = <<python[test_url]
import json

url = NAAB_VAR_test_url
result = {"status": "ok", "url": url}
json.dumps(result)
    >>

    print("Python returned: ", result, "\n")
    print("Test passed!\n")
}
