use io

fn greet(name) {
    return "Hello, " + name
}

fn calculate(x, y) {
    if x > y {
        return x + y
    } else {
        return x - y
    }
}

fn processArray(items) {
    let total = 0
    for item in items {
        total = total + item
    }
    return total
}

main {
    let greeting = greet("Alice")
    io.write(greeting + "\n")

    let numbers = [1, 2, 3, 4, 5]
    let sum = processArray(numbers)
    io.write("Sum: " + sum + "\n")

    let result = calculate(10, 5)
    io.write("Result: " + result + "\n")
}
