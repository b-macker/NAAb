// Test data transformer

use io
use json
use web_scraper

main {
    io.write("Testing data transformer\n")

    // Create a test ScrapedItem
    let test_item = new web_scraper.ScrapedItem {
        name: "Test Item",
        description: "Test Description",
        raw_html_snippet: "<div>test</div>"
    }

    let items = [test_item]
    io.write("Created test items\n")

    // Serialize to JSON
    let json_str = json.stringify(items)
    io.write("JSON: ", json_str, "\n")
}
