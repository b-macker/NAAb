// Integration Test: Import aliases
// Tests: import {func as alias}, name conflict resolution

import {add as sum, multiply as product} from "./modules/math_utils.naab"

main {
    // Test 1: Aliased import
    let result1 = sum(7, 8)
    if result1 != 15 {
        throw "FAIL: sum(7, 8) should return 15, got " + result1
    }
    print("PASS: aliased import (add as sum)")

    // Test 2: Multiple aliases
    let result2 = product(3, 4)
    if result2 != 12 {
        throw "FAIL: product(3, 4) should return 12, got " + result2
    }
    print("PASS: multiple aliased imports")

    // Test 3: Aliases prevent name conflicts
    let add = 100
    let result3 = sum(5, 5)
    if result3 != 10 {
        throw "FAIL: alias should prevent name conflict, got " + result3
    }
    if add != 100 {
        throw "FAIL: local variable should be unchanged, got " + add
    }
    print("PASS: aliases prevent name conflicts")

    print("All alias tests passed")
}
