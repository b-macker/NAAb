// Integration Test: Wildcard imports and aliasing
// Tests: import * as, import with aliases

import * as math from "./modules/math_utils.naab"

main {
    // Test 1: Wildcard import
    let result1 = math.add(10, 20)
    if result1 != 30 {
        throw "FAIL: math.add(10, 20) should return 30, got " + result1
    }
    print("PASS: wildcard import works")

    // Test 2: Access multiple functions via namespace
    let result2 = math.multiply(5, 6)
    if result2 != 30 {
        throw "FAIL: math.multiply(5, 6) should return 30, got " + result2
    }
    print("PASS: namespace access to multiple functions")

    // Test 3: Nested calls with namespace
    let result3 = math.divide(math.power(2, 3), 2)
    if result3 != 4 {
        throw "FAIL: math.divide(math.power(2, 3), 2) should return 4, got " + result3
    }
    print("PASS: nested namespace function calls")

    print("All wildcard import tests passed")
}
