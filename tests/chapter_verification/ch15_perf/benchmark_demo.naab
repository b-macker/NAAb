# Chapter 15: Performance Verification

use time as time

main {
    print(">>> Chapter 15 Verification: Performance <<<")
    
    let start = time.now_millis()
    
    let i = 0
    while i < 100 {
        i = i + 1
    }
    
    let end = time.now_millis()
    print("Loop took:", end - start, "ms")
    
    print(">>> Chapter 15 Verification: COMPLETE <<<")
}
