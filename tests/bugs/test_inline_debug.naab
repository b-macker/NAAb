main {
    print("Test 1: Simple Python inline (should work)")
    let x = <<python
2 + 2
>>
    print("  Result:", x)

    print("Test 2: Python inline with variable binding")
    let y = 10
    print("  About to execute Python inline with [y] binding...")
    let result = <<python[y]
y * 2
>>
    print("  Result:", result)

    print("Test 3: Multiple variable binding")
    let a = 5
    let b = 3
    let sum = <<python[a, b]
a + b
>>
    print("  Sum:", sum)
}
