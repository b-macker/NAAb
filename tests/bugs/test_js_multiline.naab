// Test: JavaScript multi-line code evaluation
// Isolates the IIFE wrapping fix for JS code with const/let declarations

main {
    print("=== JS Multi-line Test ===")

    // Test 1: Simple single-line JS (should work)
    let js_simple = <<javascript
    10 + 20
    >>
    print("Test 1 - Simple:", js_simple)

    // Test 2: Multi-line JS with const declarations
    let js_multi = <<javascript
    const a = 10;
    const b = 20;
    a * b
    >>
    print("Test 2 - Multi-line:", js_multi)

    // Test 3: Multi-line JS with let declarations
    let js_let = <<javascript
    let x = 5;
    let y = 3;
    x + y
    >>
    print("Test 3 - Let:", js_let)

    print("=== All JS tests complete ===")
}
